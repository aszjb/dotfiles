# main()
snippet cl
	${1}:after {
		content: '';
		display: block;
		clear: both;
		height:0;
	}
snippet dbox
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: -o-box;
	display: box;
snippet boxs
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	-o-box-sizing: border-box;
	box-sizing: border-box;
