[user]
	email = k.hokamura@gmail.com
	name = Kazuhito Hokamura
[color]
	ui = auto
	status = auto
	diff = auto
	branche = auto
[alias]
	co = checkout
	ci = commit -v
	st = status
	br = branch
	l  = log --date=iso -10
	amend = commit --amend -C HEAD
	graph = log --graph --pretty=oneline
	a = add -A .
	pr = pull --rebase
[push]
	default = simple
[core]
	excludesfile = ~/.gitexclude
	pager = LESS=-R less
	editor = /usr/bin/vim
	autocrlf = false
[github]
	user = hokaccha
[heroku]
	account = personal
